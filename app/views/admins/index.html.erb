<p id="notice"><%= notice %></p>

<h1>Listando Administradores</h1>

<table>
  <thead>
    <tr>
      <th>Rut</th>
      <th>Nombre</th>
      <th>Telefono</th>
      <th>Direccion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admins.each do |admin| %>
      <tr>
        <td><%= admin.rut %></td>
        <td><%= admin.nombre %></td>
        <td><%= admin.telefono %></td>
        <td><%= admin.direccion %></td>
        <td><%= link_to 'Mostrar', admin %></td>
        <td><%= link_to 'Editar', edit_admin_path(admin) %></td>
        <td><%= link_to 'Eliminar', admin, method: :delete, data: { confirm: 'EstÃ¡ Seguro/a?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

